<?parsers version="1.0"?>
<xs:schema
        xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="airports">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="airport" minOccurs="0" maxOccurs="200">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="airportId" type="xs:int"/>
                            <xs:element name="airportName" type="xs:string"/>
                            <xs:element name="iataCode" type="xs:string"/>
                            <xs:element name="cityId" type="xs:int"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>